local v_u_6 = game.ReplicatedStorage:FindFirstChild("BanHammerActivate")
fire_Delay = 0. -- put this how many yo u want
while true do
if  v_u_6:IsA('RemoteEvent') then
v_u_6:FireServer()
task.wait(fire_Delay) -- do not remove it will just crash ur client and not the server
  end
end
--[[
You need to execute it a bunch of times 
for it to actually start lagging the server


--]]
